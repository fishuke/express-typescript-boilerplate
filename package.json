{
  "name": "express-typescript-boilerplate",
  "version": "1.0.0",
  "description": "Production-ready Express.js boilerplate with TypeScript, TypeDI, routing-controllers, class-validator, OpenAPI documentation, and auto-generated client SDK using Kubb",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/app.js",
    "generate:openapi": "ts-node src/utils/generateOpenApiSpec.ts",
    "generate:client": "kubb generate",
    "generate": "npm run generate:openapi && npm run generate:client",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "express",
    "typescript",
    "typedi",
    "routing-controllers",
    "class-validator",
    "openapi",
    "swagger",
    "kubb",
    "boilerplate",
    "rest-api",
    "dependency-injection"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@kubb/plugin-client": "^4.5.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "class-validator-jsonschema": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.10.4",
    "routing-controllers-openapi": "^4.0.0",
    "swagger-ui-express": "^5.0.1",
    "typedi": "^0.10.0",
    "uuid": "^10.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@kubb/cli": "^2.26.2",
    "@kubb/core": "^2.26.2",
    "@kubb/swagger": "^2.26.2",
    "@kubb/swagger-client": "^2.26.2",
    "@kubb/swagger-tanstack-query": "^2.26.2",
    "@kubb/swagger-ts": "^2.26.2",
    "@kubb/swagger-zod": "^2.26.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.1",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
